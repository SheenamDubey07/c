#include <stdio.h>

int main(){
    int t,n,rem,rev=0;
        printf("Enter the num: ");
        scanf("%d",&n);
        t = n;
        while(t != 0)
    {
       
        rem = t%10;
            rev = rev * 10+rem;
            t /= 10;
    }
    printf("reverse: %d\n",rev);
    if(n == rev)
        printf("Pal");
    else
        printf("Not pal");
    return 0;
}
